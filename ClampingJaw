#include <Servo.h>            //加载文件库
int pos = 0;
Servo myservo;
 
void setup()
{
  myservo.attach(9, 500, 2500);          //修正脉冲宽度
}
 
void loop()
{
  for (pos = 42; pos <= 120; pos += 2) {       //pos+=1等价于pos=pos+1
    myservo.write(pos);
    delay(15);					
  }
  for (pos = 120; pos >= 42; pos -= 2) {
    myservo.write(pos);
    delay(15); 					
  }
}
